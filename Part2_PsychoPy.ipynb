      "data_fname = exp_info['participant'] + '_' + exp_info['date']\n",
      "data_fname = os.path.join(datapath, data_fname)\n",
      "\n",
      "\n",
      "#========================\n",
      "# Prepare condition lists\n",
      "#========================\n",
      "\n",
      "# Check if all images exist\n",
