      "            'age':'', \n",
      "            'left-handed':False \n",
      "            }\n",
      "dlg = gui.DlgFromDict(dictionary=exp_info, title=exp_name)\n",
      "\n",
      "# If 'Cancel' is pressed, quit\n",
      "if dlg.OK == False:\n",
      "    core.quit()\n",
